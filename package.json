{
    "name": "todo",
    "version": "0.1.0",
    "description": "group stats",
    "keywords": [
        "TODO",
        "TODO"
    ],
    "main": "server.js",
    "scripts": {
        "test": "mocha --compilers js:babel-register",
        "start": "nodemon --trace-deprecation src/server.js --exec babel-node",
        "debug": "nodemon --debug src/server.js --exec babel-node",
        "build": "export NODE_ENV=production&& babel src -d dist",
        "serve": "export NODE_ENV=production&& node dist/server.js",
        "buildW": "set NODE_ENV=production&& babel src -d dist",
        "serveW": "set NODE_ENV=production&& node dist/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thebix/TODO.git"
    },
    "author": "Paul Byron",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/thebix/TODO/issues"
    },
    "homepage": "https://github.com/thebix/TODO#readme",
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-2": "^6.18.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-config-airbnb-base": "^11.2.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "5.0.1",
        "eslint-plugin-react": "^7.0.1",
        "forever": "~0.15.3",
        "mocha": "^3.2.0",
        "nodemon": "^1.11.0"
    },
    "dependencies": {
        "rx": "~4.1.0",
        "redux": "^3.6.0",
        "jsonfile": "^2.4.0",
        "babel-register": "^6.18.0"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "sourceMaps": true,
        "retainLines": true
    }
}